<body>
	<%
	Cookie[] cookies = request.getCookies();
	for (int i=0; i < cookies.length; i++){
		out.println("id : "+cookies[i].getName()+ "에 대한 로그아웃을 시도합니다." + cookies[i].getValue());
		cookies[i].setMaxAge(0);
		response.addCookie(cookies[i]);
	}
	%>
</body>
</html>